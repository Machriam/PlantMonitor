{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"tabWidth": 4,
	"bracketSameLine": true,
	"arrowParens": "always",
	"bracketSpacing": false,
	"endOfLine": "lf",
	"proseWrap": "preserve",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
